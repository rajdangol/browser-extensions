{
    "manifest_version": 2,
    "name": "Quick Notes",
    "version": "1.0",
    "description": "Adds a note",
    "icons": {
        "48": "icons/note.png"
    },
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_icon": "icons/note.png",
        "default_title": "Quick Notes",
        "default_popup": "popup/notes.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "quicknote-example@mozilla.org"
        }
    }
}